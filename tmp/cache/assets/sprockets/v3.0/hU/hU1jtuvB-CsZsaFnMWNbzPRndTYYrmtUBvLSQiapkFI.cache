I"y(function() {
  jQuery(function() {
    var classrooms, students;
    $('.note_div').hide();
    $('#_users_id').parent().hide();
    classrooms = $('#_classroom_id').html();
    students = $('#_student_id').html();
    $('#_classroom_id').prepend("<option value='todos'>Todas</option>");
    $('#_classroom_id option:first').attr("selected", "selected");
    $('#_student_id').prepend("<option value='todos'>Todos</option>");
    $('#_student_id option:first').attr("selected", "selected");
    return $('#_classroom_id').on("change", function() {
      var classroom, options;
      classroom = $('#_classroom_id option:selected').text();
      options = $(students).filter("optgroup[label='" + classroom + "']").html();
      console.log(options);
      if (options) {
        $('#_student_id').html(options);
        $('#_student_id').prepend("<option value='todos'>Todos</option>");
        $('#_student_id option:first').attr("selected", "selected");
        return $('#_student_id').parent().show();
      } else {
        $('#_student_id').empty();
        return $('#_student_id').parent().hide();
      }
    });
  });

}).call(this);
