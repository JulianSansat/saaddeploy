    return $('#user_institution_id').on("change", function() {
      var institution, options;
      institution = $('#user_institution_id option:selected').text();
      options = $(courses).filter("optgroup[label='" + institution + "']").html();
      if (options) {
        return $('#user_course_id').html(options);
      } else {
        return $('#user_course_id').empty();
      }
    });
  });

}).call(this);
