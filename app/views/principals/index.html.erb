<h1>Listando Diretores</h1>

<table>
  <thead>
    <tr>
      <th>Foto</th>
      <th>Nome</th>
      <th>Telefone</th>
      <th>Endere√ßo</th>
      <th>Biografia</th>
      <th>Matricula</th>
      <th>Email</th>
      <th>Autorizado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @principals.each do |principal| %>
      <tr>
        <td><%= image_tag principal.avatar.url(:thumb) %></td>
        <td><%= principal.nome %></td>
        <td><%= principal.telefone %></td>
        <td><%= principal.endereco %></td>
        <td><%= principal.biografia %></td>
        <td><%= principal.matricula %></td>
        <td><%= principal.email %></td>
        <td><%= principal.approved %></td>
        <td><%= link_to 'Mostrar', principal %></td>
        <td><%= link_to 'Editar', edit_principal_path(principal) %></td>
        <td><%= link_to 'Deletar', principal, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

