var categories = ['Experiência Concreta', 'Observação Reflexiva', 'Conceituação Abstrata', 'Experimentação Ativa'],
    count = 0;

$('#graphic').highcharts({

            chart: {
            polar: true
        },

        title: {
            text: 'Estilos de Aprendizagem'
        },

        pane: {
            startAngle: 0
        },


        xAxis: {
            tickInterval: 1,
            min: 0,
            max: 4,
            tickmarkPlacement: 'on',
            categories: categories,
        },


        yAxis: {
            min: 0,
            max: 46
        },

        plotOptions: {
            series: {
                pointStart: 0,
                pointInterval: 1,
                 pointPlacement: 'on'
            },
            column: {
                pointPadding: 0,
                groupPadding: 0,
            }
        },

        series: [{
            type: 'area',
            name: '<%= @selecao %>',
