$('#list').html('<%= escape_javascript render (@learning_results) %>');

var content = []

$('#exports').empty();
<% if @learning_results.size == 1 %>
     pdf = '<%= link_to "Gerar PDF", pdf_list_learning_results_path(format: "pdf", result: @learning_results.first) %>';
      $('#exports').append(pdf);
<%end%>

$('#graphic').highcharts({

            chart: {
            polar: true
        },

        title: {
            text: 'Highcharts Polar Chart'
        },

        pane: {
            startAngle: 0,
            endAngle: 360
        },

        xAxis: {
            tickInterval: 90,
            min: 0,
            max: 360,
            categories: ['Experiência Concreta', 'Observação Reflexiva', 'Conceituação Abstrata', 'Experimentação']
        },

        yAxis: {
            min: 0
        },

        plotOptions: {
            series: {
                pointStart: 0,
                pointInterval: 90
            },
            column: {
                pointPadding: 0,
                groupPadding: 0
            }
        },



        series: [{
            type: 'area',
            name: 'Area',
            data: [<%= @mediaEc %>,<%= @mediaOr %>, <%= @mediaCa%>,<%= @mediaAc %>]
        }]

});