<h2><i class="fa fa-angle-right"></i>Minhas Turmas</h2>

<div class="row mt">
  <div class="col-lg-12">
      <div class="content-panel">
        <div class="row mt">
                <div class="col-lg-12">
                      <div>
                          <div class="unseen">
                            <div class="dados">
                          <table class="table table-bordered table-striped table-condensed">
                            <thead>
                              <tr>
                                <th>Disciplina</th>
                                <th>Ano</th>
                                <th>Semestre</th>
                                <th>Turma</th>
                                <th>Turno</th>
                                <th>Ações</th>
                              </tr>
                            </thead>
                            <tbody>
                              <% @classrooms.each do |classroom| %>
                                <tr>
                                  <td><%= classroom.subject.nome %></td>
                                  <td><%= classroom.ano %></td>
                                  <td><%= classroom.semestre %></td>
                                  <td><%= classroom.codigo %></td>
                                  <td><%= classroom.turno%></td>
                                  <td><center><%= button_to 'ver', classroom, :class => "btn btn-primary btn-sm", method: :get%></center></td>
                                </tr>
                              <% end %>
                            </tbody>
                          </table>
                          </div>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </div>
  </div>



<br>
