<h1>Listing Coordinators</h1>

<table>
  <thead>
    <tr>
      <th>Foto</th>
      <th>Nome</th>
      <th>Telefone</th>
      <th>Endere√ßo</th>
      <th>Biografia</th>
      <th>Matricula</th>
      <th>Email</th>
      <th>Autorizado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coordinators.each do |coordinator| %>
      <tr>
         <td><%= image_tag coordinator.avatar.url(:thumb) %></td>
        <td><%= coordinator.nome %></td>
        <td><%= coordinator.telefone %></td>
        <td><%= coordinator.endereco %></td>
        <td><%= coordinator.biografia %></td>
        <td><%= coordinator.matricula %></td>
        <td><%= coordinator.email %></td>
        <td><%= coordinator.approved %></td>
        <td><%= link_to 'Show', coordinator %></td>
        <td><%= link_to 'Edit', edit_coordinator_path(coordinator) %></td>
        <td><%= link_to 'Destroy', coordinator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

